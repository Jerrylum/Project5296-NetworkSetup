{
  "name": "project5296-network-setup",
  "version": "0.0.1",
  "description": "The network setup tool for project 5296",
  "main": "dist/src/index.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src/**/*.ts --exec ts-node ./src/app.ts",
    "build": "tsc",
    "create": "ts-node ./src/create.ts",
    "destroy": "ts-node ./src/destroy.ts",
    "lint": "eslint --fix --quiet src/**/*.ts",
    "format": "prettier --loglevel silent --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.504.0",
    "dotenv": "^16.4.1",
    "minimist": "^1.2.8"
